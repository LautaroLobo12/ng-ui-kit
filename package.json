{
  "name": "demo-lib",
  "version": "0.1.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/bitlogic-eng/ng-ui-kit.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build bitlogic --prod",
    "test": "ng test",
    "lint": "ng lint",
    "symlink": "npm link ./dist/bitlogic",
    "build:dev": "ng build bitlogic",
    "build:dev:w": "ng build bitlogic --watch",
    "publish": "npm publish ./dist/bitlogic",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "deploy-storybook": "npx angular-cli-ghpages --dir=storybook-static",
    "prebuild-ucc": "node-sass --importer ./node_modules/node-sass-package-importer/dist/cli.js ./projects/bitlogic/assets/scss/theme.scss ./projects/bitlogic/assets/prebuild-themes/theme-ucc.css --output-style compressed",
    "prebuild-u21": "node-sass --importer ./node_modules/node-sass-package-importer/dist/cli.js ./projects/bitlogic/assets/scss/theme.scss ./projects/bitlogic/assets/prebuild-themes/theme-u21.css --output-style compressed",
    "version:pat": "cd ./projects/bitlogic && npm version patch",
    "version:min": "cd ./projects/bitlogic && npm version minor",
    "version:maj": "cd ./projects/bitlogic && npm version major"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.1.1",
    "@angular/cdk": "13.1.1",
    "@angular/common": "13.1.1",
    "@angular/compiler": "13.1.1",
    "@angular/core": "13.1.1",
    "@angular/forms": "13.1.1",
    "@angular/material": "13.1.1",
    "@angular/platform-browser": "13.1.1",
    "@angular/platform-browser-dynamic": "13.1.1",
    "@angular/router": "13.1.1",
    "@ngu/carousel": "^3.0.2",
    "@nrwl/workspace": "^10.3.0",
    "bootstrap": "^4.6.0",
    "concurrently": "^5.3.0",
    "fuse.js": "^6.4.6",
    "hammerjs": "^2.0.8",
    "ng-packagr": "^13.1.2",
    "node-sass": "^7.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.2",
    "@angular/cli": "13.1.2",
    "@angular/compiler-cli": "13.1.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "serve": "^11.3.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.4",
    "ng-packagr": "^13.0.0"
  }
}
